{
  "name": "codegradxlib",
  "version": "2.0.10",
  "description": "Javascript Library to interact with the CodeGradX infrastructure. Runs on Node.js or within browsers",
  "keywords": [
    "CodeGradX",
    "Infrastructure for mechanically grading programs",
    "Paracamplus",
    "browserify-able"
  ],
  "main": "codegradxlib.js",
  "scripts": {
    "lint": "jshint codegradxlib.js spec/*.js",
    "test": "jasmine",
    "doc": "node_modules/.bin/jsdoc -c conf.json codegradxlib.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChristianQueinnec/CodeGradXlib.git"
  },
  "dependencies": {
    "he": "^1.1.1",
    "lodash": "^4.17.10",
    "minimatch": "^3.0.4",
    "rest": "^1.3.2",
    "sax": "^1.2.4",
    "when": "^3.7.8",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "gulp": "^4.0.0",
    "jasmine": "^2.3.2",
    "jshint": "^2.8",
    "nsp": "^3.2.1"
  },
  "files": [
    "package.json",
    "README.md",
    "codegradxlib.js",
    ".gitignore",
    "Makefile",
    "spec/*-spec.js",
    "spec/org.example.fw4ex.grading.check.tgz",
    "spec/vmauthor-data.js",
    "spec/min.c",
    "spec/fw4ex.xml",
    "spec/oefgc.tgz",
    "spec/oefgc/fw4ex.xml",
    "spec/support/jasmine.json"
  ],
  "author": {
    "name": "Christian Queinnec",
    "email": "Christian.Queinnec@paracamplus.com",
    "url": "http://christian.queinnec.org/"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/ChristianQueinnec/CodeGradXlib/issues"
  },
  "homepage": "https://github.com/ChristianQueinnec/CodeGradXlib#readme",
  "tonicExampleFilename": "tonicExample.js"
}

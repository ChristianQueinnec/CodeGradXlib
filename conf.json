{
  "opts": {
    "template": "./node_modules/jsdoc3-bootstrap",
    "encoding": "utf8",
    "destination": "./doc/"
    },
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
      "plugins/markdown",
      "plugins/summarize"
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "outputSourceFiles": false
    }
}
